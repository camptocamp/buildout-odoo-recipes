case $$1 in
    start)
         ($supervisor_daemon_path) || echo "Daemon probably already started"
         $supervisor_command_path start all
         ;;

    stop)
         $supervisor_command_path stop all
         ;;
    restart)
         ($supervisor_daemon_path) || echo  "Daemon probably already started"
         $supervisor_command_path restart all
         ;;
    status)
         $supervisor_command_path status
         ;;
    *)
         echo "Usage: $$0 {start|stop|restart|status} [version ..]"
         exit 1
         ;;
esac
